<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆形头像框属性-->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <!--验证码控件属性-->
    <declare-styleable name="VerificationCodeView">
        <attr name="text" format="string" />
        <attr name="text_length" format="integer" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="view_background" format="color" />
    </declare-styleable>

    <!--带有边框的九宫格属性-->
    <declare-styleable name="FramedGridView">
        <attr name="gridview_border_width" format="dimension" />
        <attr name="gridview_border_color" format="color" />
    </declare-styleable>

</resources>